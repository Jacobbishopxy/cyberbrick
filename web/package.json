{
  "name": "cyberbrick",
  "version": "0.0.0",
  "private": true,
  "description": "Cyber Brick",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "tsc": "tsc"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.14.2",
    "@ant-design/pro-form": "^1.30.0",
    "@ant-design/pro-layout": "^6.19.7",
    "@ant-design/pro-list": "^1.10.2",
    "@ant-design/pro-table": "^2.43.0",
    "@handsontable/react": "^9.0.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/typeorm": "^7.1.5",
    "@opuscapita/react-filemanager": "^1.1.10",
    "@opuscapita/react-filemanager-connector-node-v1": "^1.1.10",
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.16.5",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "classnames": "^2.3.1",
    "echarts": "^5.1.2",
    "echarts-for-react": "^3.0.1",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "handsontable": "^9.0.1",
    "highcharts": "^9.3.0",
    "highcharts-react-official": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nest-router": "^1.0.9",
    "pg": "^8.6.0",
    "rc-util": "^5.13.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.2.5",
    "react-quill": "^2.0.0-beta.1",
    "reflect-metadata": "^0.1.13",
    "save": "^2.4.0",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.34",
    "umi": "^3.4.25"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@types/classnames": "^2.3.1",
    "@types/echarts": "^4.9.8",
    "@types/express": "^4.17.12",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.8",
    "@types/react-grid-layout": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-ant-design-pro": "^1.3.2",
    "@umijs/preset-react": "^1.8.13",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^3.2.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
