{
  "name": "cyberbrick",
  "version": "0.0.0",
  "private": true,
  "description": "Cyber Brick",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build:backend": "nest build",
    "build:frontend": "umi build",
    "build": "nest build && umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d frontend",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "tsc": "tsc",
    "serve:dev": "cross-env ts-node-dev --project tsconfig.server.json server/main.ts",
    "serve": "cross-env NODE_ENV=production ts-node backend/main"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-layout": "^6.5.14",
    "@handsontable/react": "^4.0.0",
    "@nestjs/common": "^7.5.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.5.2",
    "@nestjs/platform-express": "^7.5.2",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/typeorm": "^7.1.4",
    "antd": "^4.8.4",
    "axios": "^0.21.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "handsontable": "^8.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "nest-router": "^1.0.9",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-grid-layout": "^1.1.1",
    "react-quill": "^2.0.0-beta.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "umi": "^3.2.27",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@nestjs/cli": "^7.5.3",
    "@nestjs/schematics": "^7.2.2",
    "@types/classnames": "^2.2.11",
    "@types/echarts": "^4.9.0",
    "@types/express": "^4.17.9",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/react": "^16.9.56",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.9",
    "@types/react-grid-layout": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "@umijs/fabric": "^2.3.1",
    "@umijs/plugin-blocks": "^2.2.1",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.6.19",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
